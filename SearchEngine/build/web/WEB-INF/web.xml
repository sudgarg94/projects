<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Controller</servlet-name>
        <servlet-class>com.servlet.Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OnLoad</servlet-name>
        <servlet-class>com.servlet.OnLoad</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>PrintingIndex</servlet-name>
        <servlet-class>com.servlet.PrintingIndex</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrecisionRecall</servlet-name>
        <servlet-class>com.servlet.PrecisionRecall</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controller</servlet-name>
        <url-pattern>/Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OnLoad</servlet-name>
        <url-pattern>/OnLoad</url-pattern>
        <load-on-startup>1</load-on-startup>
        <init-param>
            <param-name>contextConfigLocation</param-name>
            <param-value>/WEB-INF/spring-servlet.xml</param-value>
        </init-param>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrintingIndex</servlet-name>
        <url-pattern>/PrintingIndex</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrecisionRecall</servlet-name>
        <url-pattern>/PrecisionRecall</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
